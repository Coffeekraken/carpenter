{
	"presets": [["env", {
		"targets": {
			"browsers": [
				"Chrome >= 52",
				"FireFox >= 44",
				"Safari >= 9",
				"Explorer 11",
				"last 4 Edge versions"
			]
		}
  }]],
  "plugins": [
		"transform-class-properties",
		"transform-object-rest-spread",
		"transform-export-default",
		"add-module-exports"
  ]
}
