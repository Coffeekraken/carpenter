{
  "name": "coffeekraken-carpenter",
  "version": "0.0.1",
  "description": "Powerful tool that provide a nice web interface to display your website styleguide and modules/plugins documentations in one place.",
  "main": "dist/index.js",
  "homepage": "http://coffeekraken.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeekraken/carpenter"
  },
  "bin": {
    "coffeekraken-carpenter": "./bin/coffeekraken-carpenter"
  },
  "scripts": {
    "assets": "npm run assets.css && npm run assets.js && npm run assets.img",
    "assets.css": "node-sass public/assets-src/sass/style.scss public/assets/css/style.css --include-path node_modules --output-style expanded",
    "assets.js": "webpack",
    "assets.img": "imagemin public/assets-src/img/* --out-dir=public/assets/img",
    "assets.watch": "concurrently --kill-others \"npm run assets.watch.css\" \"npm run assets.watch.js\"",
    "assets.watch.css": "nodemon -w public/assets-src -e scss -x \"npm run assets.css\"",
    "assets.watch.js": "nodemon -w public/assets-src -e js -x \"npm run assets.js\"",
    "assets.watch.img": "nodemon -w public/assets-src/img  -x \"npm run assets.img\"",
    "dist": "babel src -d dist",
    "dist.watch": "nodemon -w src -e js -x \"npm run dist\"",
    "start": "concurrently --kill-others \"npm run assets.watch\" \"npm run dist.watch\" \"nodemon ./bin/coffeekraken-carpenter\"",
    "test": "coffeekraken-testing-stack"
  },
  "keywords": [
    "styleguide",
    "interface",
    "web",
    "node",
    "js",
    "npm",
    "express",
    "doc",
    "customize",
    "sugar",
    "coffeekraken",
    "coffee",
    "kraken"
  ],
  "author": "Coffeekraken <hello@coffeekraken.io>",
  "contributors": [{
    "name": "Olivier Bossel",
    "email": "olivier.bossel@gmail.com",
    "url": "http://olivierbossel.com"
  }],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "codemirror": "^5.22.0",
    "coffeekraken-gridle": "https://github.com/Coffeekraken/gridle.git#release/2.0.49",
    "coffeekraken-s-custom-scrollbar-component": "https://github.com/Coffeekraken/s-custom-scrollbar-component.git#release/0.0.1",
    "coffeekraken-s-interactive-demo-component": "https://github.com/Coffeekraken/s-interactive-demo-component.git#release/0.0.1",
    "coffeekraken-s-activate-component": "https://github.com/Coffeekraken/s-activate-component.git#release/0.0.1",
    "coffeekraken-s-ripple-component": "https://github.com/Coffeekraken/s-ripple-component.git#release/0.0.1",
    "coffeekraken-s-trianglify-component": "https://github.com/Coffeekraken/s-trianglify-component.git#release/0.0.1",
    "coffeekraken-sugar": "https://github.com/Coffeekraken/sugar.git#release/0.0.1",
    "coffeekraken-compile-server": "https://github.com/Coffeekraken/compile-server.git#release/0.0.1",
    "coffeekraken-docblock-parser": "https://github.com/Coffeekraken/docblock-parser.git#release/0.0.1",
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "express-handlebars": "^3.0.0",
    "fs-readdir-recursive": "^1.0.0",
    "glob": "^7.1.1",
    "glob-all": "^3.1.0",
    "handlebars": "^4.0.6",
    "imagemin": "^5.2.2",
    "lodash": "^4.17.2",
    "marked": "^0.3.6",
    "material-prism-theme": "^0.1.0",
    "open": "0.0.5",
    "prismjs": "^1.6.0",
    "webcomponents.js": "^0.7.23"
  }
}
